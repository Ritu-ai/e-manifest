{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"description": "Schema for auth Service Response",
	"$ref": "#/definitions/Auth",
	"definitions": {
		"Auth": {
			"type": "object",
			"properties": {
				"token": {
					"type": "string"
				},
				"expiration": {
					"type": "string",
					"format": "date-time"
				}
			},
			"required": [
				"token",
				"expiration"
			],
			"additionalProperties": false
		}
	}
}